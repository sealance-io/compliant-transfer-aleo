{
  "name": "@sealance-io/policy-engine-aleo",
  "version": "0.1.2",
  "description": "SDK for generating Merkle proofs and interacting with Aleo compliance policy programs",
  "author": "Sealance Team",
  "license": "Apache-2.0",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "tsc",
    "clean": "rimraf dist coverage",
    "prebuild": "npm run clean",
    "format": "prettier --check \"src/**/*.ts\" \"test/**/*.ts\"",
    "format:fix": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "prepublishOnly": "npm run build",
    "build:examples": "npm run build && cd examples && npm install --ignore-scripts",
    "publish:github": "npm publish --registry=https://npm.pkg.github.com",
    "publish:npm": "npm publish --registry=https://registry.npmjs.org"
  },
  "keywords": [
    "aleo",
    "blockchain",
    "merkle-tree",
    "zero-knowledge",
    "compliance",
    "privacy"
  ],
  "dependencies": {
    "@scure/base": "^2.0.0"
  },
  "devDependencies": {
    "@types/node": "^24.10.9",
    "@vitest/coverage-v8": "^4.0.17",
    "prettier": "^3.8.0",
    "rimraf": "^6.1.2",
    "typescript": "^5.9.3",
    "vitest": "^4.0.17"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sealance-io/compliant-transfer-aleo.git",
    "directory": "packages/policy-engine-sdk"
  },
  "peerDependencies": {
    "@provablehq/sdk": "^0.9.14"
  }
}
