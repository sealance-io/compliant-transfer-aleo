{
  "transform": {
    "^.+\\.(t|j)sx?$": ["ts-jest", {
      "useESM": true,
      "tsconfig": {
        "module": "NodeNext"
      }
    }]
  },
  "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
  "moduleNameMapper": {
    "^(\\.{1,2}/.*)\\.js$": "$1"
  },
  "moduleFileExtensions": ["ts", "tsx", "js", "jsx", "json", "node"],
  "preset": "ts-jest/presets/js-with-ts-esm",
  "globalTeardown": "./jest.global-teardown.js",
  "setupFilesAfterEnv": ["./jest.setup.js"],
  "testTimeout": 3000000,
  "workerThreads": true
}